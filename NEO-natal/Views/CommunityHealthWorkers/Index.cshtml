@model IEnumerable<NEO_natal.Models.CommunityHealthWorker>

@{
    ViewBag.Title = "Community Health Worker";
}
<div style="height: 100px;"></div> <!-- this so the banner doesn't cover the page content -->

<h1>Community Health Worker</h1>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.loginName)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.firstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.lastName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.loginName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.firstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.lastName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.communityHealthWorkerID }) |
            @Html.ActionLink("Details", "Details", new { id=item.communityHealthWorkerID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.communityHealthWorkerID })
        </td>
    </tr>
}

</table>
